                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <Space
                android:layout_width="20dp"
                android:layout_height="match_parent"
                android:layout_weight="0" />

            <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CheckBox" />

            <Space
                android:layout_width="20dp"
